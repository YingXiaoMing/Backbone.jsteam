<script>
$(document).ready(function(e) {
	$(".productSearch .type a").click(function(e) {
      var id=$(this).attr("id");
			a=$(".select_model ul");
			$(this).addClass("selected");
			$(this).prevAll().removeClass("selected");
			$(this).nextAll().removeClass("selected");
		if(id==0){
			a.find("li").show();
		}
		else{
			a.find("li").each(function(index, element) {
        var b=$(this).attr("id");
				if(id==b){
					$(this).show();
				}
				else{
					$(this).hide();
				}
      });
		}
  });
	$(".productSearch .select_model ul li").click(function(e) {
    $(this).addClass("selected");
		$(this).prevAll().removeClass("selected");
		$(this).nextAll().removeClass("selected");
		$("#${field.name}").val($(this).attr('data-type'));
  });
});
</script>
